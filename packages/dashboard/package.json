{
  "name": "dashboard",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "dotenv -- remix-serve ./build/server/index.js",
    "pm2-start": "pm2 start pm2.config.cjs",
    "pm2-stop": "pm2 stop pm2.config.cjs",
    "typecheck": "tsc",
    "lint": "biome lint --write --diagnostic-level=error src/*",
    "format": "biome check --write --linter-enabled=false src/*",
    "lint-and-format": "biome check --write --diagnostic-level=error src/*"
  },
  "dependencies": {
    "@holiday-jp/holiday_jp": "^2.4.0",
    "@remix-run/node": "^2.15.0",
    "@remix-run/react": "^2.15.0",
    "@remix-run/serve": "^2.15.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "googleapis": "^144.0.0",
    "isbot": "^4.1.0",
    "playwright": "^1.49.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.14.1",
    "tailwind-merge": "^2.5.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.15.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
